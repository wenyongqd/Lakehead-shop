
.list{
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 20rpx;
	border-bottom: #F4F4F4 solid 1rpx;
}
.rounded-circle{
	-webkit-border-radius: 15%;
	        border-radius: 15%;
	margin-right: 10rpx;
	border: #171606 solid 1rpx;
}
.list1{
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	-webkit-border-radius: 35%;
	        border-radius: 35%;
}
.list2{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-border-radius: 15%;
	        border-radius: 15%;
}
.list3{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.text-m{
	font-size: 30rpx;
}
.text-secondary{
	color: #797b80;
	font-size: 25rpx;
}

